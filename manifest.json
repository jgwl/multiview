{
  "author": "Jarrett Lee",
  "manifest_version": 2,
  "name": "Multiview",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"]
    }
  ],
  "description": "Opens URL twice in two panes on the same page.",
  "icons": {
    "16": "multiview.svg",
    "48": "multiview.svg",
    "96": "multiview.svg"
  },
  "page_action": {
    "browser_style": true,
    "default_icon": {
      "16": "multiview.svg",
      "24": "multiview.svg"
    },
    "default_title": "Multiview"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}   